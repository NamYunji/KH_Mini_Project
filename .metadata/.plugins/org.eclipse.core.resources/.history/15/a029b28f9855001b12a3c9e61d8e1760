package view;

import java.io.Serializable;


	
public class Voca1 implements Serializable{
	
	//필드
	private int num;
	private String word;
	private String meaning;

	

	//생성자
	public Voca1() {
		super();
		// TODO Auto-generated constructor stub
	}
	

	public Voca1(int num, String word, String meaning) {
		super();
		this.num = num;
		this.word = word;
		this.meaning = meaning;
	}


	//getter & setter
	public int getNum() {
		return num;
	}


	public void setNum(int num) {
		this.num = num;
	}


	public String getWord() {
		return word;
	}


	public void setWord(String word) {
		this.word = word;
	}


	public String getMeaning() {
		return meaning;
	}


	public void setMeaning(String meaning) {
		this.meaning = meaning;
	}

	//toString
	
	@Override
	public String toString() {
		return "Voca1 [num=" + num + ", word=" + word + ", meaning=" + meaning + "]";
	}
	




	
	
	
	
	
	

}



